
import time
import random
chemins=[]
import math
monstres=[]
def chemin():
     y1=random.randint(10,60)
     x1=random.randint(5,10)
   
     for s in range(x1):
      chemins.append((s,y1))
      x=x1
     chemins.append((x, y1)) 
      
    
    
     while x<=105:
       
       add=random.randint(5,10)
       if y1<30:
        
        for m in range(add):
         y1+=1
         chemins.append((x,y1))
       else:
        
        for m in range(add):
         y1-=1
         chemins.append((x,y1))
       for n in range(random.randint(5, 10)):
            x += 1
            chemins.append((x, y1))
     xfin=120-x
     if y1<32:
      yfin=32-y1
      for k in range(yfin):
       y1+=1
       chemins.append((x,y1))

     else:
      yfin=abs(32-y1)
      for k in range(yfin):
        y1-=1
        chemins.append((x,y1))
     for n in range(xfin):
            x += 1
            chemins.append((x,y1))
chemin()
print(chemins)
def monstre():
          
          x=time()
    
       
          time.sleep(5)
          
         
          y=time()
          dif=y-x
          if dif==4:
             monstres.append([0, 32])

chemin()
print(monstres)

      
       
       
     
    

    

       

        


