import random
chemins=[]
def chemin():
    y1=random.randint(10,60)
    x1=random.randint(5,10)
    t=random.randint(5,10)
    for s in range(1,x1):
     chemins.append((s,y1))
    for e in range(1,t):
       x=x1
       if y1<30:
        add=random.randint(5,10)
        for m in range(add):
         y1+=1
         chemins.append((x,y1))
       else:
        add=random.randint(5,10)
        for m in range(add):
         y1-=1
         chemins.append((x,y1))
       for n in range(random.randint(5, 10)):
            x += 1
            chemins.append((x, y1))
    
chemin()
print(chemins)


      
       
       
     
    

    

       

        


